#include<bits/stdc++.h>
using namespace std;
int BFS(int start,int end, int n,int arr[])
{
	//step 1 have a queue
	if (start==end)return 0;
	queue<pair<int,int>>Q;
	Q.push({start,0});
	while(!Q.empty())
	{
		auto it=Q.front();
		int newstart = it.first;
		int steps = it.second;
		Q.pop();
		for(int i=0; i<n; i++)
		{
			if(newstart*arr[i]==end)
			{
				return steps+1;

			}
			else
			{
				if (newstart*arr[i]%100000<end)
				{
					Q.push({newstart*arr[i]%100000,steps+1});
				}
			}
		}
	}
	return -1;
} 
int main()
{
	int start,end;
	cin>>start>>end;
	int n;cin>>n;
	int arr[n];
	for (int i=0;i<n;i++)
		{
			cin>>arr[i];
		}
	cout<<BFS(start,end,n,arr);
}


